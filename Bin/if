PROGRAM if;
VAR i: INTEGER;
BEGIN
i:=4;

IF i < 10 THEN
WRITE 1;
ELSE 
WRITE 0;
ENDIF;

IF i > 10 THEN
WRITE 0;
ELSE 
WRITE 1;
ENDIF;

IF i <= 4 THEN
WRITE 1;
ELSE 
WRITE 0;
ENDIF;

IF i >= 4 THEN
WRITE 1;
ELSE 
WRITE 0;
ENDIF;

IF i = 4 THEN
WRITE 1;
ELSE 
WRITE 0;
ENDIF;

IF i != 4 THEN
WRITE 0;
ELSE 
WRITE 1;
ENDIF;

IF i = 4 THEN
WRITE 1337;
ELSE
ENDIF;

END.